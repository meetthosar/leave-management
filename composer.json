{
    "name": "karyam/leave-management",
    "description": "This package will manage leaves in organization. Depends upon laravel auth and role package",
    "keywords": [
        "karyam",
        "leave-management"
    ],
    "homepage": "https://github.com/meetthosar/leave-management",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Meet Thosar",
            "email": "meetthosar@karyam.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Karyam\\LeaveManagement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karyam\\LeaveManagement\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Karyam\\LeaveManagement\\LeaveManagementServiceProvider"
            ],
            "aliases": {
                "LeaveManagement": "Karyam\\LeaveManagement\\LeaveManagementFacade"
            }
        }
    },
     "minimum-stability": "stable",
    "prefer-stable": true
}
